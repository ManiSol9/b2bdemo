      <div className="settings_heading">Machine Parameters</div>
      <table><thead className="settings_thead"><tr><th></th><th>Min</th><th>Max</th></tr></thead><tbody className="settings_tbody">
        {
          this.state.checkboxes.map((item,index) => (
            <tr key={item.key}>
            <td>
              <Checkbox name={item.name} checked={this.state.checkedItems.get(item.name)} onChange={(evt) => this.handleChange(evt,index,item.key)} />
             <label className="settings_label"> {item.name}</label></td>
            <td><input  type="text"  value={item.min} onBlur={(evt) => this.handleBlurMin(evt,index,item.key)} onChange={(evt) => this.handleChangedMin(evt,index,item.key)}/></td>
            <td><input  type="text"  value={item.max} onBlur={(evt) => this.handleBlurMax(evt,index,item.key)} onChange={(evt) => this.handleChangedMax(evt,index,item.key)}/></td>
            </tr>
          ))
        }
        </tbody></table>
        <button type="button" className="settings_button" onClick={this.save}>Save</button>